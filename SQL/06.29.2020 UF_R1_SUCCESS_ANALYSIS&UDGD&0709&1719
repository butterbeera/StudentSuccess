CREATE TABLE UF_R1_SUCCESS_ANALYSIS AS(SELECT
    
A.ACAD_CAR_SID,
A.INSTITUTION_SID,
A.STDNT_CAR_NBR,
A.TERM_SID,
A.STRM,
A.TERM_BEG_DT_SID,
A.TERM_END_DT_SID,
A.DATE_OF_BIRTH,
A.INSTITUTION,
A.ACAD_CAREER,
A.AGE_YEARS,
A.AGE_MONTHS,
A.AGE_DAYS,
A.TOT_CUMULATIVE,
A.JUNIOR_SENIOR_FLAG,
A.TOT_TAKEN_PRGRSS,
A.TOT_TRNSFR,
A.TOT_TEST_CREDIT,
A.TOT_OTHER,
A.TOT_PASSD_PRGRSS,
A.UNT_TAKEN_PRGRSS,
A.CUR_GPA,
A.CUM_GPA,
A.ENRL_CNT,
A.ENRL_FLG,
A.SSR_TRMAC_LAST_DT,
A.ACAD_LEVEL_BOT,
A.ACAD_LEVEL_EOT,
A.UF_CLASS,
A.RESIDENCY,
A.LASTUPD_EW_DTTM,
A.PROF_GRAD_FLAG,
A.ACADEMIC_LOAD,
A.TOT_GRADE_POINTS,
A.TOT_TAKEN_GPA,
A.ENRL_SUMMER_A_FLAG,
A.ENRL_SUMMER_B_FLAG,
A.ENRL_SUMMER_C_FLAG,
A.ACAD_PROG_PRIMARY,
A.UF_CLASS_EOT,
A.UNT_TAKEN_GPA,
A.UNT_INPROG_GPA,
A.TERM_FIRST_APPT_TIME,
A.TERM_BEG_DT_SID_FALL_CATGRY,
A.TERM_BEG_DT_SID_SPRING_CATGRY,
A.TERM_END_DT_SID_FALL_EARLY,
A.TERM_END_DT_SID_SPRING_EARLY,
A.TERM_LENGTH_CATEGORY,
A.TERM_LENGTH_DAYS,
A.TERM_SEASON,
B.PERSON_SID,
B.LOW_TERM_GPA_IND,
B.PARTTIME_TERM_IND,
B.NOT_REG_TERM_IND,
B.WITHDRWL_TERM_IND,
B.FULLTIME_TERM_IND,
B.OVR_12HR_TERM_IND
    FROM UF_SUCCESS_TARGET B FULL OUTER JOIN UF_B_STDNT_TERM A 
    ON A.PERSON_SID = B.PERSON_SID
)


CREATE TABLE UF_R1_SUCCESS_ANALYSIS_0709 AS(SELECT
* from UF_R1_SUCCESS_ANALYSIS
  WHERE TERM_BEG_DT_SID>20070000 AND TERM_BEG_DT_SID<20091231
)

CREATE TABLE UF_R1_SUCCESS_ANALYSIS_1719 AS(SELECT
* from UF_R1_SUCCESS_ANALYSIS
  WHERE TERM_BEG_DT_SID>20170000 AND TERM_BEG_DT_SID<20191231
)

CREATE TABLE UF_R1_SUCCESS_ANALYSIS_UGRD AS(SELECT
* from UF_R1_SUCCESS_ANALYSIS
  WHERE ACAD_CAR_SID = 8
)
